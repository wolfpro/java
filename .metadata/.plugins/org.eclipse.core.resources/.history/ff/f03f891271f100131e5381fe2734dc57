import java.io.*;
import java.util.*;

public class Main {
	static FileWriter fo;

	public static boolean ts(char c) {// True symbol;
		if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z')
				|| (c >= 'А' && c <= 'я')) {
			return true;
		}
		return false;
	}

	public static boolean tsRus(char c) {// True symbol;
		if ((c >= 'А' && c <= 'я' || c == ' ')) {
			return true;
		}
		return false;
	}

	public static String check(String st) {
		StringBuilder s = new StringBuilder(), sd = new StringBuilder();
		char as, bs;
		as = st.charAt(0);

		int size = st.length();
		boolean ad = true;
		s.append(as);
		for (int i = 1; i < size; i++) {
			bs = st.charAt(i);
			if (bs == '–' || bs == '-')
				ad = false;
			if (bs == ';') {
				ad = true;
				s.append(' ');
			}
			if (bs != '	')
				if (ad)
					s.append(bs);
			if (bs == '	')
				if (ad)
					s.append(' ');
			as = bs;
		}
		size = s.length();
		ad = true;
		for (int i = 0; i < size; ++i) {
			bs = s.charAt(i);
			if (bs == ';')
				break;
			if (bs == ',') {
				ad = false;
				sd.append(" ");
			}
			if (ad == false)
				if (tsRus(bs))
					sd.append(bs);
			if (ad)
				sd.append(bs);
		}
		s = sd;
		sd = new StringBuilder();
		size = s.length();
		as = s.charAt(0);
		bs = ' ';
		for (int i = 1; i < size; i++) {
			bs = s.charAt(i);
			if (!(as == ' ' && bs == ' ') && !(as == ' ' && bs == '.')
					&& !(as == ' ' && bs == '\n'))
				sd.append(as);
			as = bs;
		}
		if (bs != ' ')
			sd.append(bs);
		s = sd;

		return s.toString();
	}

	public static void main(String[] args) throws IOException {

		File fl = new File("MyDictionary.txt");
		File fl1 = new File("asd");
		InputStream f;
		fo = new FileWriter(fl1);

		f = new FileInputStream(fl);
		Scanner sc = new Scanner(f);
		String s;
		try {
			while ((s = sc.nextLine()) != null) {
				s = check(s);
				fo.write(s + '\n');
				// if (s.charAt(0)>'a')
				System.out.println(s);

			}
		} catch (NoSuchElementException e) {
			System.out.println();
		}
	}
}