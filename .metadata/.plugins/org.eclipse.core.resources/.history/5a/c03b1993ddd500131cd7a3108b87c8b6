import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int month = sc.nextInt();
		String season;
		switch (month) {
		case 1:
		case 12:
		case 2:
			season = "Зима(Winter)";
			break;
		case 3:
		case 4:
		case 5:
			
		default:
			break;
		}

	}
}
