
import java.io.File;

public class FileManager {
	File fl;
	String dirname="/examples/";
	
	FileManager() {		
		fl = new File(dirname);
		if (fl.isDirectory()) {
			System.out.println("Dir name "+dirname);
			String s[]=fl.list();
			for(int i=0; i<s.length; i++){
				System.out.println(s[i]);
			}
		}
	}
}
